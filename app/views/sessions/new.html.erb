
<%= form_tag url_for(action: 'create'), method: "post" do %>

    <%= label_tag 'email' %>
    <%= text_field_tag 'email', @user.email %>
    <br /> <br />

    <%= label_tag 'password' %>
    <%= text_field_tag 'password', @user.password %>
    <br /> <br />

    <%= submit_tag "enter" %>

<%end%> 